{
  "extends": ["react-app", "prettier", "prettier/react"],
  "plugins": ["prettier", "react", "react-hooks"],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "error",
    "no-unused-vars": "error",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "arrowParens": "always"
      }
    ]
  }
}
